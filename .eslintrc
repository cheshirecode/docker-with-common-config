{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "globals": {
    "process": true,
    "module": true,
    "window": true,
    "require": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": [
    "babel",
    "react",
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "complexity": "error",
    "linebreak-style": 0,
    "require-jsdoc": 0,
    "spaced-comment": 0,
    "no-invalid-this": 0,
    "babel/no-invalid-this": "error",
    "react/display-name": 0,
    "jsx-a11y/anchor-is-valid": 0
  }
}
